    "version": "0.2.0",
    "configurations": [
    {
        "name": "Launch via NPM",
        "request": "launch",
        "runtimeArgs": [
            "run-script",
            "debug"
        ],
        "runtimeExecutable": "npm",
        "skipFiles": [
            "<node_internals>/**"
        ],
        "type": "node"
    },
        {
            "type": "node",
            "request": "launch",
            "name": "Run tsx",
            "runtimeExecutable": "tsx",
            "cwd": "${workspaceFolder}",
            "args": []
        },
        {
            "type": "node-terminal",
            "name": "Run Script: dev",
            "request": "launch",
            "command": "npm run dev",
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "node-terminal",
            "name": "Run Script: dev2",
            "request": "launch",
            "command": "npm run dev2",
            "cwd": "${workspaceFolder}"
        }
    ]
}